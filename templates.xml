<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
   <template description="3 Round MM handicap tournament." name="Monthly Ratings">
   <Header>
      <PairingFormat>MCMAHON</PairingFormat>
      <Rules>JAPANESE</Rules>
      <BasicTime>0</BasicTime>
      <OvertimeFormat>
         <Canadian StoneCount="25" TimeBlockSize="300"/>
      </OvertimeFormat>
      <TiebreakSystemList>
         <System Method="MMS" Tier="1"/>
         <System Method="SOSM" Tier="2"/>
         <System Method="SOSOSM" Tier="3"/>
         <System Method="NULL" Tier="4"/>
         <System Method="NULL" Tier="5"/>
         <System Method="NULL" Tier="6"/>
      </TiebreakSystemList>
   </Header>
   <eggplant>
      <numberofrounds>3</numberofrounds>
      <mmbar>1D</mmbar>
      <mmfloor>20K</mmfloor>
      <bandspacingscheme>SPACED</bandspacingscheme>
      <lastroundforseedsystem>1</lastroundforseedsystem>
      <pairingscheme1>SPLIT_AND_SLIP</pairingscheme1>
      <pairingscheme2>SPLIT_AND_SLIP</pairingscheme2>
      <handicapgames>true</handicapgames>
      <maxhandicappedrank>9D</maxhandicappedrank>
      <handicapbasis>RANK</handicapbasis>
      <handicapmodifier>0</handicapmodifier>
      <handicapceiling>9</handicapceiling>
      <defaultkomi>6</defaultkomi>
      <absentnbwx2>0</absentnbwx2>
      <absentmmsx2>0</absentmmsx2>
      <byenbwx2>1</byenbwx2>
      <byemmsx2>1</byemmsx2>
   </eggplant>
</template>
